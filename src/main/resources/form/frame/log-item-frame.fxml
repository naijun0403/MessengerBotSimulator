<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>

<?import org.beuwi.msgbots.platform.gui.control.Label?>
<?import org.beuwi.msgbots.platform.gui.layout.HBox?>
<?import org.beuwi.msgbots.platform.gui.layout.StackPane?>
<?import org.beuwi.msgbots.platform.gui.layout.VBox?>

<HBox fx:id="hbxBoxRoot" styleClass="root-pane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
	<children>
		<StackPane alignment="TOP_CENTER">
			<children>
				<ImageView fx:id="imvBoxIcon" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true">
					<StackPane.margin>
						<Insets top="0.0" right="10.0" left="5.0"></Insets>
					</StackPane.margin>
				</ImageView>
			</children>
		</StackPane>
		<VBox HBox.hgrow="ALWAYS">
			<children>
				<Label fx:id="lblBoxText" VBox.vgrow="ALWAYS"></Label>
				<Label fx:id="lblBoxDate"></Label>
			</children>
		</VBox>
	</children>
	<padding>
		<Insets top="10.0" right="5.0" bottom="10.0" left="5.0"></Insets>
	</padding>
</HBox>